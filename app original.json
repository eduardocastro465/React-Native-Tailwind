{
  "expo": {
    "name": "AtelierLookSocial",
    "slug": "atelier-movil",
    "owner": "eduardoryt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/atr.png",
    "scheme": "ateliermovil",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.eduardoryt.ateliermovil",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/atr.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",

      [
        "expo-camera",
        {
          "cameraPermission": "Esta app necesita acceder a tu c√°mara para tomar fotos."
        }
      ],

      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],

      "expo-web-browser"
    ],
    "web": {
      "output": "static",
      "favicon": "./assets/atr.png"
    },

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "67653622-0813-43c6-bfd2-1495aef28755"
      }
    },

    "cli": {
      "appVersionSource": "source"
    }
  }
}
